{
  "version": "1.0",
  "description": "Simple audio example with Introspector",
  
  "mods": {
    "Audio": {
      "type": "AudioDataSource",
      "config": {
        "MinPitch": "50.0",
        "MaxPitch": "2500.0"
      }
    },
    "Introspector": {
      "type": "Introspector"
    }
  },
  
  "connections": [
    "Audio.PitchRmsPoint -> Introspector.Point",
    "Audio.ComplexSpectralDifferenceScalar -> Introspector.HorizontalLine1",
    "Audio.SpectralCrestScalar -> Introspector.HorizontalLine2",
    "Audio.ZeroCrossingRateScalar -> Introspector.HorizontalLine3"
  ]
}
