{
  "version": "1.0",
  "description": "Reference of all Mod types with default parameter values. Copy/paste entries to create new Synth configurations.",

  "synth": {
    "agency": 0,
    "manualBiasDecaySec": 0.8,
    "baseManualBias": 0.1,
    "backgroundColor": "0, 0, 0, 1",
    "backgroundMultiplier": 0.1,
    "crossfadeDelaySec": 0.5,
    "crossfadeDuration": 2.0,
    "initialIntent": {
      "strength": 0.0,
      "activations": []
    },
    "_comment_agency": "agency: 0.0 = fully manual control, 1.0 = fully autonomous",
    "_comment_manualBiasDecaySec": "manualBiasDecaySec: Time (0.1-5.0s) for manual control influence to decay after releasing controls",
    "_comment_baseManualBias": "baseManualBias: Minimum manual control influence (0.0-0.5) that persists after decay",
    "_comment_crossfadeDelaySec": "crossfadeDelaySec: Hold time (seconds) to keep showing the old composite snapshot before crossfade begins",
    "_comment_crossfadeDuration": "crossfadeDuration: Duration (0.5-10.0s) of crossfade transition when switching configs",
    "_comment_initialIntent": "initialIntent: Initial intent configuration applied on load",
    "_comment_initialIntent_strength": "initialIntent.strength: Global intent multiplier (0.0-1.0), scales all intent effects. Default 0.0",
    "_comment_initialIntent_activations": "initialIntent.activations: Array of initial activation values (0.0-1.0) for each intent by index. Unspecified intents default to 0.0. Extra values are ignored with a warning."
  },

  "drawingLayers": {
    "_comment": "Drawing layers are FBOs that mods can draw to. They are composited in order.",
    "ExampleLayer": {
      "size": [1080, 1080],
      "internalFormat": "GL_RGBA32F",
      "wrap": "GL_CLAMP_TO_EDGE",
      "clearOnUpdate": false,
      "blendMode": "OF_BLENDMODE_ALPHA",
      "useStencil": false,
      "numSamples": 0,
      "isDrawn": true,
      "isOverlay": false,
      "alpha": 1.0,
      "paused": false,
      "description": "Short human-readable description of what this layer does.",
      "_comment_size": "size: [width, height] dimensions of the FBO",
      "_comment_internalFormat": "internalFormat: GL_RGBA, GL_RGB, GL_RGBA32F, GL_RGB32F, GL_RG32F, GL_RGBA16F, GL_RGB16F, GL_RG16F, GL_RGBA8, GL_RGB8",
      "_comment_wrap": "wrap: GL_CLAMP_TO_EDGE, GL_REPEAT, GL_MIRRORED_REPEAT",
      "_comment_clearOnUpdate": "clearOnUpdate: true to clear the FBO each frame, false to accumulate",
      "_comment_blendMode": "blendMode: OF_BLENDMODE_DISABLED, OF_BLENDMODE_ALPHA, OF_BLENDMODE_ADD, OF_BLENDMODE_SUBTRACT, OF_BLENDMODE_MULTIPLY, OF_BLENDMODE_SCREEN",
      "_comment_useStencil": "useStencil: true to enable stencil buffer",
      "_comment_numSamples": "numSamples: MSAA samples (0 = disabled)",
      "_comment_isDrawn": "isDrawn: true to include in final composite",
      "_comment_isOverlay": "isOverlay: true to render after base layers (can sample current composite)",
      "_comment_alpha": "alpha: Initial alpha (0.0-1.0) for this layer in the composite. Controllable via GUI slider.",
      "_comment_paused": "paused: true to start this layer in a paused (faded-out) state; it will still render its FBO when unpaused, fading to and from its alpha value.",
      "_comment_description": "description: Optional human-readable description shown in GUI tooltips for this layer."
    }
  },

  "mods": {

    "_comment_source_mods": "=== SOURCE MODS ===",

    "ExampleAudioDataSource": {
      "type": "AudioDataSource",
      "_requires": "ResourceManager with ('sourceAudioPath', 'audioOutDeviceName', 'audioBufferSize', 'audioChannels', 'audioSampleRate', optional 'sourceAudioStartPosition') OR ('micDeviceName', 'recordAudio', 'audioRecordingPath')",
      "_comment_sourceAudioStartPosition": "sourceAudioStartPosition: Optional start timestamp in MM:SS format (e.g. '1:30' for 1 minute 30 seconds)",
      "config": {
        "MinPitch": "50",
        "MaxPitch": "800",
        "MinRms": "0",
        "MaxRms": "0.02",
        "MinComplexSpectralDifference": "20",
        "MaxComplexSpectralDifference": "70",
        "MinSpectralCrest": "20",
        "MaxSpectralCrest": "100",
        "MinZeroCrossingRate": "5",
        "MaxZeroCrossingRate": "15",
        "OnsetThreshold": "2.0",
        "OnsetCooldownSecs": "2.0",
        "TimbreThreshold": "2.0",
        "TimbreCooldownSecs": "2.0",
        "PitchThreshold": "2.0",
        "PitchCooldownSecs": "2.0",
        "_comment_event_detection": "Event detection parameters: Threshold (0.5-5.0) controls sensitivity (lower = more events). CooldownSecs (0.0-30.0) is minimum time between events of that type."
      }
    },

    "ExampleStaticTextSource": {
      "type": "StaticTextSource",
      "config": {
        "Text": "",
        "EmitOnce": "1",
        "Delay": "0"
      }
    },

    "ExampleTextSource": {
      "type": "TextSource",
      "_requires": "ResourceManager with 'textSourcesPath'",
      "config": {
        "TextFilename": "text.txt",
        "ParseWords": "0",
        "Randomness": "0",
        "Loop": "1",
        "AgencyFactor": "1",
        "_comment_Randomness": "Randomness: 0.0 = sequential, 1.0 = random, 0.5 = 50% chance of each. Both modes draw from unvisited items only.",
        "_comment_Loop": "Loop: When false, stops emitting after each item emitted exactly once (works for sequential and random). When toggled true, resets immediately."
      }
    },

    "ExampleTimerSource": {
      "type": "TimerSource",
      "config": {
        "Interval": "1",
        "Enabled": "1",
        "OneShot": "0",
        "TimeToNext": "0",
        "AgencyFactor": "1"
